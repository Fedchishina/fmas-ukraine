  object SQLListBox: TListBox
    Left = 16
    Top = 344
    Width = 57
    Height = 57
    ItemHeight = 13
    Items.Strings = (
      'SET TERM ^ ;'
      ''
      'CREATE TRIGGER :Table_Name_AIU FOR :Table_Name'
      'ACTIVE AFTER INSERT OR UPDATE OR DELETE POSITION 0'
      'AS'
      'DECLARE VARIABLE LOC_USER_FIO_HST VARCHAR(255);'
      'DECLARE VARIABLE LOC_DATE_TIME_HST TIMESTAMP;'
      'DECLARE VARIABLE LOC_ACTION_HST VARCHAR(20);'
      'DECLARE VARIABLE LOC_ID_HISTORY_INFO_HST NUMERIC(16,0);'
      'BEGIN'
      '/* '#1057#1086#1079#1076#1072#1085#1086' '#1075#1077#1085#1077#1088#1072#1090#1086#1088#1086#1084' */'
      ''
      
        '    /* '#1055#1086#1083#1091#1095#1072#1077#1084' '#1080#1085#1092#1086#1088#1084#1072#1094#1080#1102' '#1086' '#1087#1086#1083#1100#1079#1086#1074#1072#1090#1077#1083#1077', '#1082#1086#1090#1086#1088#1099#1081' '#1074#1099#1087#1086#1083#1085#1080#1083' '#1080#1079#1084#1077 +
        #1085#1077#1085#1080#1077' */'
      '    SELECT H.USER_FIO, H.DATE_TIME, H.ID_HISTORY_INFO'
      '    FROM   PUB_DT_HISTORY_INFO H'
      '    WHERE  H.ID_TRANSACTION            = CURRENT_TRANSACTION'
      
        '           AND H.DATE_TIME = (SELECT MAX(DATE_TIME) FROM PUB_DT_' +
        'HISTORY_INFO'
      
        '                              WHERE ID_TRANSACTION = CURRENT_TRA' +
        'NSACTION)'
      
        '    INTO   :LOC_USER_FIO_HST, :LOC_DATE_TIME_HST, :LOC_ID_HISTOR' +
        'Y_INFO_HST;'
      ''
      '    /* '#1048#1085#1080#1094#1080#1072#1083#1080#1079#1080#1088#1091#1077#1084' '#1080#1085#1092#1086#1088#1084#1072#1094#1080#1102' '#1086' '#1087#1086#1083#1100#1079#1086#1074#1072#1090#1077#1083#1077' */'
      '    IF ( LOC_ID_HISTORY_INFO_HST IS NULL )'
      '    THEN BEGIN'
      '              LOC_USER_FIO_HST  = '#39'Unknown'#39';'
      '              LOC_DATE_TIME_HST = CURRENT_TIMESTAMP;'
      '    END'
      ''
      '    /* '#1055#1086#1083#1091#1095#1072#1077#1084' '#1090#1080#1087' '#1080#1079#1084#1077#1085#1077#1085#1080#1103' */'
      '    IF ( INSERTING ) THEN'
      '    BEGIN'
      '        SELECT H.DESC_ACTION'
      '        FROM   PUB_SYS_HST_ACTIONS H'
      '        WHERE  H.NUM_ACTION = 1'
      '        INTO   :LOC_ACTION_HST;'
      ''
      ''
      
        '         IF ( LOC_ACTION_HST IS NULL )  THEN LOC_ACTION_HST = '#39#1044 +
        #1086#1073#1072#1074#1083#1077#1085#1072#39';'
      '    END'
      ''
      '    IF ( UPDATING ) THEN'
      '    BEGIN'
      '        SELECT H.DESC_ACTION'
      '        FROM   PUB_SYS_HST_ACTIONS H'
      '        WHERE  H.NUM_ACTION = 2'
      '        INTO   :LOC_ACTION_HST;'
      ''
      ''
      
        '         IF ( LOC_ACTION_HST IS NULL )  THEN LOC_ACTION_HST = '#39#1048 +
        #1079#1084#1077#1085#1077#1085#1072#39';'
      '    END'
      ''
      '    IF ( DELETING ) THEN'
      '    BEGIN'
      '        SELECT H.DESC_ACTION'
      '        FROM   PUB_SYS_HST_ACTIONS H'
      '        WHERE  H.NUM_ACTION = 3'
      '        INTO   :LOC_ACTION_HST;'
      ''
      ''
      
        '         IF ( LOC_ACTION_HST IS NULL )  THEN LOC_ACTION_HST = '#39#1059 +
        #1076#1072#1083#1077#1085#1072#39';'
      '    END'
      ''
      ''
      ''
      '    /* '#1055#1088#1086#1090#1086#1082#1086#1083#1080#1088#1091#1077#1084' '#1080#1079#1084#1077#1085#1077#1085#1080#1077' */'
      '    IF ( INSERTING ) THEN'
      '    INSERT INTO :Hst_Table_Name ('
      '        USER_FIO_HST,'
      '        DATE_TIME_HST,'
      '        ACTION_HST,'
      '        ID_HISTORY_INFO_HST,'
      '        :Fields)'
      '    VALUES ('
      '        :LOC_USER_FIO_HST,'
      '        :LOC_DATE_TIME_HST,'
      '        :LOC_ACTION_HST,'
      '        :LOC_ID_HISTORY_INFO_HST,'
      '        :New_Fields);'
      '    ELSE'
      '    INSERT INTO :Hst_Table_Name ('
      '        USER_FIO_HST,'
      '        DATE_TIME_HST,'
      '        ACTION_HST,'
      '        ID_HISTORY_INFO_HST,'
      '        :Fields)'
      '    VALUES ('
      '        :LOC_USER_FIO_HST,'
      '        :LOC_DATE_TIME_HST,'
      '        :LOC_ACTION_HST,'
      '        :LOC_ID_HISTORY_INFO_HST,'
      '        :Old_Fields);'
      'END'
      '^'
      ''
      'SET TERM ; ^'
      ''
      'CREATE GENERATOR Gen_:Hst_Table_Name;'
      ''
      'SET TERM ^ ;'
      ''
      'CREATE OR ALTER TRIGGER :Hst_Table_Name_Bi FOR :Hst_Table_Name'
      'ACTIVE BEFORE INSERT POSITION 0'
      'AS'
      'BEGIN'
      ''
      
        '    /* '#1047#1072#1087#1086#1083#1085#1103#1077#1084' '#1082#1086#1088#1088#1077#1082#1090#1085#1099#1084' '#1079#1085#1072#1095#1077#1085#1080#1077#1084' '#1087#1086#1083#1077' '#1076#1083#1103' '#1087#1077#1088#1074#1080#1095#1085#1086#1075#1086' '#1082#1083#1102#1095#1072' ' +
        '*/'
      '    IF ( NEW.Id_Hst IS NULL )'
      '    THEN BEGIN'
      '              /* '#1055#1086#1083#1091#1095#1072#1077#1084' '#1079#1085#1072#1095#1077#1085#1080#1077' '#1087#1077#1088#1074#1080#1095#1085#1086#1075#1086' '#1082#1083#1102#1095#1072' */'
      
        '              EXECUTE PROCEDURE Pub_Get_Id_By_Gen('#39'Gen_:Hst_Tabl' +
        'e_Name'#39', 1 )'
      
        '              RETURNING_VALUES                   ( NEW.Id_Hst   ' +
        '                 );'
      ''
      
        '              /* '#1055#1088#1086#1074#1077#1088#1103#1077#1084' '#1091#1085#1080#1082#1072#1083#1100#1085#1086#1089#1090#1100' '#1079#1085#1072#1095#1077#1085#1080#1103' '#1087#1077#1088#1074#1080#1095#1085#1086#1075#1086' '#1082#1083#1102#1095 +
        #1072' */'
      '              WHILE ( EXISTS( SELECT *'
      '                              FROM   :Hst_Table_Name H'
      '                              WHERE  H.Id_Hst = NEW.Id_Hst ) )'
      '              DO BEGIN'
      
        '                      EXECUTE PROCEDURE Pub_Get_Id_By_Gen('#39'Gen_:' +
        'Hst_Table_Name'#39', 1 )'
      
        '                      RETURNING_VALUES                   ( NEW.I' +
        'd_Hst                    );'
      '              END'
      '    END'
      ''
      'END'
      '^'
      ''
      ''
      'SET TERM ; ^')
    TabOrder = 6
    Visible = False
  end
