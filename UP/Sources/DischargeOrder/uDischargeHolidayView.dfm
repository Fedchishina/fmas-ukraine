  object DataSet: TpFIBDataSet
    Database = frmDischargeOrder.Database
    Transaction = frmDischargeOrder.ReadTransaction
    SelectSQL.Strings = (
      
        'select p.ID_MAN_HOLIDAY_PLAN, p.used_days_count, p.days_count, p' +
        '.period_beg, p.period_end, th.full_name'
      ' from hl_dt_man_holiday_plan  p'
      
        ' join HL_SP_TYPE_HOLIDAY th on th.id_type_holiday = p.id_type_ho' +
        'liday'
      ' where used_days_count < days_count'
      
        ' and id_work_dog_moving = (select m.id_work_dog_moving from up_d' +
        't_man_moving m where m.id_man_moving=:ID_MAN_MOVING)'
