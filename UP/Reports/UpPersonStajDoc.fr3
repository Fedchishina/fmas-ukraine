 to l do&#13;&#10;   begin&#13;&#10;      s:=copy(str,i,1);&#13;&#10;      if s=' ' then fl:=false;&#13;&#10;      if fl=true then fio:=fio+s;&#13;&#10;      //showmessage(s);&#13;&#10;   end;&#13;&#10;   s:='';&#13;&#10;   fl:=true;&#13;&#10;   for i:=1 to l do&#13;&#10;   begin&#13;&#10;      if fl=true then s:=copy(str,i,1);&#13;&#10;      if s=' ' then&#13;&#10;      begin&#13;&#10;         s:=copy(str,i+1,1);&#13;&#10;         imya:=imya+s;&#13;&#10;         fl:=false;&#13;&#10;      end;&#13;&#10;   end;&#13;&#10;   k:=0;&#13;&#10;   s:='';&#13;&#10;   fl:=true;&#13;&#10;   for i:=1 to l do&#13;&#10;   begin&#13;&#10;      if fl=true then&#13;&#10;      begin&#13;&#10;         s:=copy(str,i,1);&#13;&#10;         if s=' ' then k:=k+1;&#13;&#10;         if k=2 then&#13;&#10;         begin&#13;&#10;            s:=copy(str,i+1,1);&#13;&#10;            otch:=otch+s;&#13;&#10;            fl:=false;&#13;&#10;            k:=0;&#13;&#10;         end;&#13;&#10;      end;&#13;&#10;   end;&#13;&#10;   memo4.clear;&#13;&#10;   memo4.Text:='Відповідальний за формування  ____________ '+imya+'. '+otch+'. '+fio;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo10OnBeforePrint(Sender: TfrxComponent);&#13;&#10;//var cnt:Integer;&#13;&#10;begin&#13;&#10;//  cnt:=&#60;ReportDsetData.&#34;COUNT_PEOPLE&#34;&#62;;&#13;&#10;//  CntPeople:=CntPeople+cnt;&#13;&#10;//  cnt:=0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;//var cnt:Integer;&#13;&#10;begin&#13;&#10; // cnt:=0;&#13;&#10;  //cnt:=&#60;ReportDsetData.&#34;COUNT_DAYS&#34;&#62;;&#13;&#10; // CntDays:=CntDays+cnt;&#13;&#10; // cnt:=0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo22OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var CntP:Variant;&#13;&#10;begin&#13;&#10;   //CntP:=&#60;SUM(&#60;ReportDsetData.&#34;COUNT_PEOPLE&#34;&#62;,MasterData1)&#62;;&#13;&#10;  // memo22.clear;&#13;&#10;  // memo22.Text:=VarToStr(CntP);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo23OnBefore