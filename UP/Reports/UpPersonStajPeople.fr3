portDsetData.&#34;IS_STORNO&#34;&#62;;&#13;&#10;   memo25.clear;&#13;&#10;   if is_use=1 then memo25.Text:='Так' else memo25.Text:='';  //memo25.Text:='Ні';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo27OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var str, s, fio, imya, otch:String;&#13;&#10;    i, l, k:Integer;&#13;&#10;    fl:boolean;&#13;&#10;begin&#13;&#10;   fl:=true;&#13;&#10;   fio:='';&#13;&#10;   imya:='';&#13;&#10;   otch:='';&#13;&#10;   str:=&#60;ReportDsetData.&#34;FIO_USER_MODIFY&#34;&#62;;&#13;&#10;  // showmessage(str);&#13;&#10;   l:=length(str);&#13;&#10;   for i:=1 to l do&#13;&#10;   begin&#13;&#10;      s:=copy(str,i,1);&#13;&#10;      if s=' ' then fl:=false;&#13;&#10;      if fl=true then fio:=fio+s;&#13;&#10;      //showmessage(s);&#13;&#10;   end;&#13;&#10;   s:='';&#13;&#10;   fl:=true;&#13;&#10;   for i:=1 to l do&#13;&#10;   begin&#13;&#10;      if fl=true then s:=copy(str,i,1);&#13;&#10;      if s=' ' then&#13;&#10;      begin&#13;&#10;         s:=copy(str,i+1,1);&#13;&#10;         imya:=imya+s;&#13;&#10;         fl:=false;&#13;&#10;      end;&#13;&#10;   end;&#13;&#10;   k:=0;&#13;&#10;   s:='';&#13;&#10;   fl:=true;&#13;&#10;   for i:=1 to l do&#13;&#10;   begin&#13;&#10;      if fl=true then&#13;&#10;      begin&#13;&#10;         s:=copy(str,i,1);&#13;&#10;         if s=' ' then k:=k+1;&#13;&#10;         if k=2 then&#13;&#10;         begin&#13;&#10;            s:=copy(str,i+1,1);&#13;&#10;            otch:=otch+s;&#13;&#10;            fl:=false;&#13;&#10;            k:=0;&#13;&#10;         end;&#13;&#10;      end;&#13;&#10;   end;&#13;&#10;   memo27.clear;&#13;&#10;   memo27.Text:='Відповідальний за формування  ____________ '+imya+'. '+otch+'. '+fio;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var s:String;&#13;&#10;begin&#13;&#10;   s:='';&#13;&#10;   s:=&#60;ReportDsetData.&#34;FIO&#34;&#62;;&#13;&#10;  // showmessage(s);&#13;&#10;   if s=StrFIO then&#13;&#10;   begin&#13;&#10;