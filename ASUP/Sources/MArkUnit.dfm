  object PCardViewQuery: TIBQuery
    Database = Main.ChermetDatabase
    Transaction = Main.DefaultTransaction
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'SELECT DISTINCT PC.Id_Pcard, PC.TN,  PC.Id_Man, GAF.FIO'
      ''
      
        'FROM Private_Cards PC, GET_ALL_FIOS GAF, GET_TODOLIST GT, ORDERS' +
        ' O'
      ''
      
        'WHERE PC.ID_PCARD = GAF.ID_PCARD AND GT.ID_ORDER = O.ID_ORDER AN' +
        'D GT.ID_PCARD = PC.ID_PCARD AND ID_STATUS = 5'
      ''
      'ORDER BY FIO')
