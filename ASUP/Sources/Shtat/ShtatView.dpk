  uShtatConsts in 'ShtatView\uShtatConsts.pas',
  uShtatTree in 'ShtatView\uShtatTree.pas',
  uShtatBonusFrame in 'ShtatView\uShtatBonusFrame.pas' {frmShtatBonus: TFrame},
  uShtatBonus in 'ShtatView\uShtatBonus.pas' {fmShtatBonus},
