<TfrxReport Name="" Version="3.15" DotMatrixReport="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="37886,6131762731" ReportOptions.Description.Text="" ReportOptions.LastChange="38764,425575081" ScriptLanguage="PascalScript" ScriptText.Text="uses 'Reports\GoodPageBreak.pas';&#13;&#10;&#13;&#10;var&#13;&#10;       TheTitle: String;&#13;&#10;       HeaderInd: Integer;&#13;&#10;       Department: String;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with ReportTitle1, Engine do&#13;&#10;  begin&#13;&#10;     if not FinalPass then TheTitle := &#60;ReportQuery.&#34;DEPARTMENT&#34;&#62;;&#13;&#10;&#13;&#10;     pfInit;&#13;&#10;&#13;&#10;     HeaderInd := 0;&#13;&#10;     Department := '';&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with GroupHeader1, Engine do&#13;&#10;  begin&#13;&#10;     inc(HeaderInd);&#13;&#10;&#13;&#10;     if HeaderInd = 1 then&#13;&#10;     begin&#13;&#10;       pfGroupInit;&#13;&#10;       pfGroupCheck;&#13;&#10;     end;&#13;&#10;&#13;&#10;     GroupHeader1.Font.Size := &#60;Font_Size&#62; + 1;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with MasterData1, Engine do&#13;&#10;  begin&#13;&#10;            // если итоги, убрать поле с кодом должности&#13;&#10;     if &#60;ReportQuery.&#34;Is_Totals&#34;&#62; = 1 then&#13;&#10;     begin&#13;&#10;        NamePostMemo.Left := PostCodeMemo.Left;&#13;&#10;        NamePostMemo.Font.Style := fsBold;&#13;&#10;        PostCodeMemo.Text := '';&#13;&#10;     end&#13;&#10;     else&#13;&#10;     begin&#13;&#10;         NamePostMemo.Left := PostCodeMemo.Left + PostCodeMemo.Width;&#13;&#10;         NamePostMemo.Font.Style := 0;&#13;&#10;         pfMasterCheck(Sender);&#13;&#10;         PostCodeMemo.Text := '[ReportQuery.&#34;POST_CODE&#34;]';&#13;&#10;     end;&#13;&#10;&#13;&#10;     NamePostMemo.Width := KolStavokMemo.Left - NamePostMemo.Left;&#13;&#10;&#13;&#10;     HeaderInd := 0;&#13;&#10;&#13;&#10;     MasterData1.Font.Size := &#60;Font_Size&#62;;&#13;&#10;     NoteMemo.Font.Size := &#60;Font_Size&#62;-1;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       // установить шрифта в зависимости от степени вложенности&#13;&#10;  with Memo3, Engine do&#13;&#10;  begin&#13;&#10;     if &#60;ReportQuery.&#34;NESTLEVEL&#34;&#62; = 1 then&#13;&#10;     begin&#13;&#10;        Font.Style := fsBold;&#13;&#10;     end&#13;&#10;     else if &#60;ReportQuery.&#34;NESTLEVEL&#34;&#62; = 2 then&#13;&#10;     begin&#13;&#10;          Font.Style := fsUnderline + fsBold;&#13;&#10;     end&#13;&#10;     else if &#60;ReportQuery.&#34;NESTLEVEL&#34;&#62; = 3 then&#13;&#10;     begin&#13;&#10;          Font.Style := fsItalic + fsBold;&#13;&#10;     end&#13;&#10;     else&#13;&#10;     begin&#13;&#10;         Font.Style := 0;&#13;&#10;     end;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure OkladMemoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with OkladMemo, Engine do&#13;&#10;  begin&#13;&#10;     if (&#60;Version&#62; = 1) and (&#60;ReportQuery.&#34;Id_Post&#34;&#62; = 3) then&#13;&#10;        Text := 'Контракт'&#13;&#10;     else Text := '[ReportQuery.&#34;OKLAD&#34; #9,2]';&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo7, Engine do&#13;&#10;  begin&#13;&#10;       if &#60;Vacanices&#62; = 0 then Text := 'ШТАТНИЙ РОЗПИС'&#13;&#10;       else  Text := 'ВІДОМІСТЬ ВАКАНСІЙ';&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       pfSaveSummaryHeight(Sender);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;ReportQuery.&#34;Is_Totals&#34;&#62; = 1 then&#13;&#10;         pfGroupFooterSaveHeight(Sender)&#13;&#10;  else  pfSaveMasterDataHeight(Sender);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  pfSaveGroupHeight(Sender);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnManualBuild(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;       DataSet: TfrxDataSet;&#13;&#10;begin&#13;&#10;       DataSet := MasterData1.DataSet;&#13;&#10;       DataSet.First;&#13;&#10;       while not DataSet.Eof do&#13;&#10;       begin&#13;&#10;               if ( DataSet.Value('Department') &#60;&#62; Department )&#13;&#10;                       and ( DataSet.Value('Is_Totals') = 0 ) then&#13;&#10;               begin&#13;&#10;                       Department := DataSet.Value('Department');&#13;&#10;                       Engine.ShowBand(GroupHeader1);&#13;&#10;               end;&#13;&#10;&#13;&#10;               if DataSet.Value('Is_Fake') = 0 then&#13;&#10;                       Engine.ShowBand(MasterData1);&#13;&#10;&#13;&#10;               DataSet.Next;&#13;&#10;       end;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="">
