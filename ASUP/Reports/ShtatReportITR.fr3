;{  with DigitMemo, Engine do&#13;&#10;  begin&#13;&#10;       if ( [ReportQuery.&#34;NESTLEVEL&#34;] = 0 )&#13;&#10;          or ( [ReportQuery.&#34;Kol_Stavok&#34;] &#60;= 0 ) then&#13;&#10;          Memo := ''&#13;&#10;       else Memo := [ReportQuery.&#34;DIGIT&#34;]&#13;&#10;  end}&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnManualBuild(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;       DataSet: TfrxDataSet;&#13;&#10;begin&#13;&#10;       DataSet := MasterData1.DataSet;&#13;&#10;       DataSet.First;&#13;&#10;       while not DataSet.Eof do&#13;&#10;       begin&#13;&#10;               if ( DataSet.Value('Department') &#60;&#62; Department )&#13;&#10;                       and ( DataSet.Value('Is_Totals') = 0 ) then&#13;&#10;               begin&#13;&#10;                       Department := DataSet.Value('Department');&#13;&#10;                       Engine.ShowBand(GroupHeader1);&#13;&#10;               end;&#13;&#10;&#13;&#10;               if DataSet.Value('Is_Fake') = 0 then&#13;&#10;                       Engine.ShowBand(MasterData1);&#13;&#10;&#13;&#10;               DataSet.Next;&#13;&#10;       end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterFooter1OnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  pfSaveSummaryHeight(Sender);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;ReportQuery.&#34;Is_Totals&#34;&#62; = 1 then&#13;&#10;         pfGroupFooterSaveHeight(Sender)&#13;&#10;  else  pfSaveMasterDataHeight(Sender);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  pfSaveGroupHeight(Sender);&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="">
